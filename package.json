{
  "displayName": "BeMac's Grimoire",
  "version": "1.5.0",
  "description": "A Discord bot used to provide assistance in Magic The Gathering Discord Guilds.",
  "main": "src/Grimoire.js",
  "scripts": {
    "init": "rm -rf node_modules && rm -rf flow-typed && npm install && flow-typed update",
    "winit": "RD /S /Q node_modules && RD /S /Q flow-typed && npm install && flow-typed update",
    "test": "TIMING=1 eslint --format=node_modules/eslint-formatter-pretty . && mocha --compilers js:babel-register --require babel-polyfill",
    "test-jenkins": "TIMING=1 eslint -f checkstyle -o eslint.xml . && JUNIT_REPORT_PATH=report.xml mocha --colors --compilers js:babel-register --require babel-polyfill --reporter mocha-jenkins-reporter",
    "compile-es2015": "webpack --config webpack.prod.config.js",
    "start-pm2": "pm2 start src/Grimoire.js --interpreter ./node_modules/.bin/babel-node",
    "start": "./node_modules/.bin/babel-node src/Grimoire.js"
  },
  "author": "BeMacized",
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.14.0",
    "eslint-config-airbnb-base": "latest",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-flowtype": "^2.30.1",
    "eslint-plugin-flowtype-errors": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.44.2",
    "flow-typed": "^2.0.0",
    "html-webpack-plugin": "^2.26.0",
    "mocha": "^3.2.0",
    "mocha-jenkins-reporter": "^0.3.5",
    "source-map-support": "^0.4.14",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "discord.js": "^11.0.0",
    "fs-promise": "^2.0.2",
    "lodash": "^4.17.4",
    "mtgsdk": "^0.1.1"
  }
}
